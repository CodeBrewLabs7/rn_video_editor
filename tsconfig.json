{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@navigations/*": ["src/navigations/*"],
      "@lib/*": ["src/lib/*"],
      "@typings/*": ["src/typings/*"],
    }
  }
}
